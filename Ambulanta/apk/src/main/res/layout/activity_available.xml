<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayoutAvailable"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    tools:context="com.sda.ambulanta.apk.menu.AvailableActivity">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_marginTop="50dp"
        android:gravity="center_horizontal"
        android:layout_centerInParent="true">
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal">
            <Button
                android:id="@+id/availableYesBTN"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:background="@drawable/btn_color_sos"
                android:color="#f8fdf9"
                android:text="Disponibil"
                android:textAppearance="@style/CalcResultStyle1" />
            <Button
                android:id="@+id/availableNoBTN"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:background="@drawable/btn_color_sos"
                android:color="#f8fdf9"
                android:text="Ocupat"
                android:textAppearance="@style/CalcResultStyle1" />
        </TableRow>
    </TableLayout>



</RelativeLayout>